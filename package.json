{
  "name": "df-fast_food-back",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "start": "node dist/server.js",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:coverage": "npm run test -- --coverage",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:seed": "npm run test:load-envs prisma db seed",
    "lint": "eslint",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "prisma": "^5.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
